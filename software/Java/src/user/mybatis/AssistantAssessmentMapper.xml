	<!-- 根据主键删除记录 -->
	<delete id="delete" parameterType="user.entity.AssistantAssessment">
		UPDATE dbo.assistant_assessment
		SET is_deleted=1
		WHERE assessment_id = #{assessmentId}
	</delete>


</mapper>