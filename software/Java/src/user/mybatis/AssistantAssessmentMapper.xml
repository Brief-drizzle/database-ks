<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="user.mapper.AssistantAssessmentMapper">
	<resultMap id="BaseResultMap" type="user.entity.AssistantAssessment">
            <result column="assessment_id" property="assessmentId" />
	        <result column="postgraduate_id" property="postgraduateId" />
	        <result column="administrator_id" property="administratorId" />
	        <result column="instructor_id" property="instructorId" />
	        <result column="self_account" property="selfAccount" />
	        <result column="instructor_evaluate" property="instructorEvaluate" />
	        <result column="evaluate_result" property="evaluateResult" />
	        <result column="examine_result" property="examineResult" />
	        <result column="examine_time" property="examineTime" />
		</resultMap>  
    
    <!-- 表字段 -->
    <sql id="baseColumns">
         t.assessment_id
        , t.postgraduate_id
        , t.administrator_id
        , t.instructor_id
        , t.self_account
        , t.instructor_evaluate
        , t.evaluate_result
        , t.examine_result
        , t.examine_time
        </sql> 
    
    <!-- 查询全部 -->
    <select id="listAll" resultMap="BaseResultMap">
		SELECT         
        	<include refid="baseColumns" />
		FROM dbo.assistant_assessment t
	</select>
 
 	<!-- 根据主键获取单条记录 -->
    <select id="getById" resultMap="BaseResultMap" parameterType="String">
		SELECT         
        	<include refid="baseColumns" />
		FROM dbo.assistant_assessment t
		WHERE assessment_id = #{assessmentId}
	</select>

	<!-- 插入全部字段 -->
    <insert id="insert" parameterType="user.entity.AssistantAssessment"
		keyProperty="assessmentId" keyColumn="assessment_id" useGeneratedKeys="true"
    >
	INSERT INTO dbo.assistant_assessment
    <trim prefix="(" suffix=")" suffixOverrides=",">	 
        	    	assessment_id,
    	        	    	postgraduate_id,
    	        	    	administrator_id,
    	        	    	instructor_id,
    	        	    	self_account,
    	        	    	instructor_evaluate,
    	        	    	evaluate_result,
    	        	    	examine_result,
    	        	    	examine_time,
    	        </trim>
    <trim prefix="VALUES (" suffix=")" suffixOverrides=",">            
        	        #{assessmentId},                   
    	        	        #{postgraduateId},                   
    	        	        #{administratorId},                   
    	        	        #{instructorId},                   
    	        	        #{selfAccount},                   
    	        	        #{instructorEvaluate},                   
    	        	        #{evaluateResult},                   
    	        	        #{examineResult},                   
    	        	        #{examineTime},                   
    	        </trim>
	</insert>
    
    <!-- 插入不为NULL的字段 -->
    <insert id="insertIgnoreNull" parameterType="user.entity.AssistantAssessment"
        keyProperty="assessmentId" keyColumn="assessment_id" useGeneratedKeys="true"
        >
        INSERT INTO dbo.assistant_assessment    
        <trim prefix="(" suffix=")" suffixOverrides=",">	 

                                                <if test="assessmentId != null">
                   assessment_id,
                    </if>
                                                                <if test="postgraduateId != null">
                   postgraduate_id,
                    </if>
                                                                <if test="administratorId != null">
                   administrator_id,
                    </if>
                                                                <if test="instructorId != null">
                   instructor_id,
                    </if>
                                                                <if test="selfAccount != null">
                   self_account,
                    </if>
                                                                <if test="instructorEvaluate != null">
                   instructor_evaluate,
                    </if>
                                                                <if test="evaluateResult != null">
                   evaluate_result,
                    </if>
                                                                <if test="examineResult != null">
                   examine_result,
                    </if>
                                                                <if test="examineTime != null">
                   examine_time,
                    </if>
                                    </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">            
                                                <if test="assessmentId != null" >
                    #{assessmentId},                    
                    </if>
                                                                <if test="postgraduateId != null" >
                    #{postgraduateId},                    
                    </if>
                                                                <if test="administratorId != null" >
                    #{administratorId},                    
                    </if>
                                                                <if test="instructorId != null" >
                    #{instructorId},                    
                    </if>
                                                                <if test="selfAccount != null" >
                    #{selfAccount},                    
                    </if>
                                                                <if test="instructorEvaluate != null" >
                    #{instructorEvaluate},                    
                    </if>
                                                                <if test="evaluateResult != null" >
                    #{evaluateResult},                    
                    </if>
                                                                <if test="examineResult != null" >
                    #{examineResult},                    
                    </if>
                                                                <if test="examineTime != null" >
                    #{examineTime},                    
                    </if>
                                    </trim>
    </insert>

	<!-- 更新,更新全部字段 -->
    <update id="update" parameterType="user.entity.AssistantAssessment">
    UPDATE dbo.assistant_assessment
     <set>		
                                     postgraduate_id=#{postgraduateId},        
                                administrator_id=#{administratorId},        
                                instructor_id=#{instructorId},        
                                self_account=#{selfAccount},        
                                instructor_evaluate=#{instructorEvaluate},        
                                evaluate_result=#{evaluateResult},        
                                examine_result=#{examineResult},        
                                examine_time=#{examineTime},        
                   </set>	
    WHERE assessment_id = #{assessmentId}
    </update>  
    
    
    <!-- 更新不为NULL的字段 -->
    <update id="updateIgnoreNull" parameterType="user.entity.AssistantAssessment">
    UPDATE dbo.assistant_assessment
    <set>
    	                                                <if test="postgraduateId != null" >
                postgraduate_id=#{postgraduateId},                 
                </if>
                                                <if test="administratorId != null" >
                administrator_id=#{administratorId},                 
                </if>
                                                <if test="instructorId != null" >
