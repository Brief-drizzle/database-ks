<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="user.mapper.PostgraduateMapper">
	<resultMap id="BaseResultMap" type="user.entity.Postgraduate">
            <result column="postgraduate_id" property="postgraduateId" />
	        <result column="teacher_id" property="teacherId" />
	        <result column="administrator_id" property="administratorId" />
	        <result column="postgraduate_name" property="postgraduateName" />
	        <result column="postgraduate_phone" property="postgraduatePhone" />
	        <result column="postgraduate_email" property="postgraduateEmail" />
	        <result column="postgraduate_birth" property="postgraduateBirth" />
	        <result column="postgraduate_sex" property="postgraduateSex" />
		</resultMap>  
    
    <!-- 表字段 -->
    <sql id="baseColumns">
         t.postgraduate_id
        , t.teacher_id
        , t.administrator_id
        , t.postgraduate_name
        , t.postgraduate_phone
        , t.postgraduate_email
        , t.postgraduate_birth
        , t.postgraduate_sex
        </sql> 
    
    <!-- 查询全部 -->
    <select id="listAll" resultMap="BaseResultMap">
		SELECT         
        	<include refid="baseColumns" />
		FROM dbo.postgraduate t
	</select>
 
 	<!-- 根据主键获取单条记录 -->
    <select id="getById" resultMap="BaseResultMap" parameterType="String">
		SELECT         
        	<include refid="baseColumns" />
		FROM dbo.postgraduate t
		WHERE postgraduate_id = #{postgraduateId}
	</select>

	<!-- 插入全部字段 -->
    <insert id="insert" parameterType="user.entity.Postgraduate"
		keyProperty="postgraduateId" keyColumn="postgraduate_id" useGeneratedKeys="true"
    >
	INSERT INTO dbo.postgraduate
    <trim prefix="(" suffix=")" suffixOverrides=",">	 
        	    	postgraduate_id,
    	        	    	teacher_id,
    	        	    	administrator_id,
    	        	    	postgraduate_name,
    	        	    	postgraduate_phone,
    	        	    	postgraduate_email,
    	        	    	postgraduate_birth,
    	        	    	postgraduate_sex,
    	        </trim>
    <trim prefix="VALUES (" suffix=")" suffixOverrides=",">            
        	        #{postgraduateId},                   
    	        	        #{teacherId},                   
    	        	        #{administratorId},                   
    	        	        #{postgraduateName},                   
    	        	        #{postgraduatePhone},                   
    	        	        #{postgraduateEmail},                   
    	        	        #{postgraduateBirth},                   
    	        	        #{postgraduateSex},                   
    	        </trim>
	</insert>
    
    <!-- 插入不为NULL的字段 -->
    <insert id="insertIgnoreNull" parameterType="user.entity.Postgraduate"
        keyProperty="postgraduateId" keyColumn="postgraduate_id" useGeneratedKeys="true"
        >
        INSERT INTO dbo.postgraduate    
        <trim prefix="(" suffix=")" suffixOverrides=",">	 

                                                <if test="postgraduateId != null">
                   postgraduate_id,
                    </if>
                                                                <if test="teacherId != null">
                   teacher_id,
                    </if>
                                                                <if test="administratorId != null">
                   administrator_id,
                    </if>
                                                                <if test="postgraduateName != null">
                   postgraduate_name,
                    </if>
                                                                <if test="postgraduatePhone != null">
                   postgraduate_phone,
                    </if>
                                                                <if test="postgraduateEmail != null">
                   postgraduate_email,
                    </if>
                                                                <if test="postgraduateBirth != null">
                   postgraduate_birth,
                    </if>
                                                                <if test="postgraduateSex != null">
                   postgraduate_sex,
                    </if>
                                    </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">            
                                                <if test="postgraduateId != null" >
                    #{postgraduateId},                    
                    </if>
                                                                <if test="teacherId != null" >
                    #{teacherId},                    
                    </if>
                                                                <if test="administratorId != null" >
                    #{administratorId},                    
                    </if>
                                                                <if test="postgraduateName != null" >
                    #{postgraduateName},                    
                    </if>
                                                                <if test="postgraduatePhone != null" >
                    #{postgraduatePhone},                    
                    </if>
                                                                <if test="postgraduateEmail != null" >
                    #{postgraduateEmail},                    
                    </if>
                                                                <if test="postgraduateBirth != null" >
                    #{postgraduateBirth},                    
                    </if>
                                                                <if test="postgraduateSex != null" >
                    #{postgraduateSex},                    
                    </if>
                                    </trim>
    </insert>

	<!-- 更新,更新全部字段 -->
    <update id="update" parameterType="user.entity.Postgraduate">
    UPDATE dbo.postgraduate
     <set>		
                                     teacher_id=#{teacherId},        
                                administrator_id=#{administratorId},        
                                postgraduate_name=#{postgraduateName},        
                                postgraduate_phone=#{postgraduatePhone},        
                                postgraduate_email=#{postgraduateEmail},        
                                postgraduate_birth=#{postgraduateBirth},        
                                postgraduate_sex=#{postgraduateSex},        
                   </set>	
    WHERE postgraduate_id = #{postgraduateId}
    </update>  
    
    
    <!-- 更新不为NULL的字段 -->
    <update id="updateIgnoreNull" parameterType="user.entity.Postgraduate">
    UPDATE dbo.postgraduate
    <set>
    	                                                <if test="teacherId != null" >
                teacher_id=#{teacherId},                 
                </if>
                                                <if test="administratorId != null" >
                administrator_id=#{administratorId},                 
                </if>
                                                <if test="postgraduateName != null" >
                postgraduate_name=#{postgraduateName},                 
                </if>
                                                <if test="postgraduatePhone != null" >
                postgraduate_phone=#{postgraduatePhone},                 
                </if>
                                                <if test="postgraduateEmail != null" >
                postgraduate_email=#{postgraduateEmail},                 
                </if>
                                                <if test="postgraduateBirth != null" >
                postgraduate_birth=#{postgraduateBirth},                 
                </if>
                                                <if test="postgraduateSex != null" >
                postgraduate_sex=#{postgraduateSex},                 
                </if>
                        </set>
    WHERE postgraduate_id = #{postgraduateId}
    </update>

		
	<!-- 根据主键删除记录 -->
	<delete id="delete" parameterType="user.entity.Postgraduate">
		UPDATE dbo.postgraduate
		SET is_deleted=1
		WHERE postgraduate_id = #{postgraduateId}
	</delete>


</mapper>